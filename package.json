{
  "name": "redux-tutorial",
  "version": "0.1.0",
  "private": true,
  "description": "redux-tutorial",
  "scripts": {
    "clean": "gulp clean:cache && gulp clean:build",
    "build": "gulp build",
    "start": "./bin/start.sh",
    "mockup-server": "mockup-server -p 4081",
    "tsc-check": "tsc --project tsconfig.json --outDir build/.tsc-check/ && tsc --project www/tsconfig.json --outDir www/build/.tsc-check/",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "eslint": "eslint --ext ts,tsx src types www/src www/types",
    "eslint:fix": "eslint --ext ts,tsx src types www/src www/types --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run stylelint && npm run tsc-check"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "closest": "^0.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.3.4",
    "express": "^4.17.1",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@mlz/webui-gulp": "0.65.0",
    "@types/core-js": "^2.5.2",
    "@types/express": "^4.17.2",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "env-status": "^0.30.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "gulp": "^3.9.0",
    "husky": "^3.0.9",
    "mockup-server": "^0.13.0",
    "prettier": "^1.19.1",
    "require-all": "^2.0.0",
    "run-sequence": "^1.1.5",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "typescript": "^3.6.4",
    "winston": "^3.2.1"
  }
}
